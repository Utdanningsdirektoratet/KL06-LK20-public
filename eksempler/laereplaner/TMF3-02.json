        "fagets-relevans": {
            "overskrift": {
                "tekst": [
                    {
                        "spraak": "default",
                        "verdi": "Fagets relevans"
                    }
                ],
                "forskrift": true
            },
            "beskrivelse": {
                "tekst": [
                    {
                        "spraak": "default",
                        "verdi": "Tømrarfaget representerer store verdiar i byggjenæringa. Faget blir drive i store og små verksemder over heile landet, og fagutøvinga varierer frå enkelt manuelt arbeid til bruk av avansert verktøy og utstyr. Tømrarfaget er eit tradisjonsrikt fag som skal vere med på å sikre estetiske og kulturelle verdiar i bransjen. Opplæringa skal medverke til å utvikle kompetanse på bygging og montering av ulike typar trekonstruksjonar og innreiingsarbeid i nybygg og ved rehabiliteringar. Opplæringa skal også vere med på å fremme sjølvstende, kreativitet, yrkesvørdnad, gode og trygge arbeidsvanar og evne til kommunikasjon med kundar, kollegaer og andre samarbeidspartnarar. Opplæringa skal leggje vekt på å utvikle fagleg fellesskap og bransjeforståing og leggje grunnlag for ei berekraftig utvikling. Opplæringa skal sikre krava til helse, miljø og tryggleik. Fullført og bestått opplæring fører fram til sveinebrev. Yrkestittel er tømrar."
                    }
                ],
                "forskrift": true
            },
            "forklaring": [
                {
                    "spraak": "default",
                    "verdi": "Forklaring på fagets relevans"
                }
            ]
        },
        "kjerneelementer-i-faget": {

    {
        "id": "uuid:c42a4874-fe34-11e8-8eb2-f2801f1b9fd1",
        "kode": "VERB20",
        "uri": "http://psi.udir.no/kl06/VERB20",
        "url-data": "http://data.udir.no/kl06/v201906/verb-lk20/VERB20",
